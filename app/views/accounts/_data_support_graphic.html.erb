<div class="isi_theme">
  <div class="col-md-4 wewof category">
    <div class="frame">
      <div class="title">
        <div class="nao pull-left">
          <span>CATEGORY</span>
        </div>
        <ul class="list-inline pull-right njw">
          <li><%= link_to category_new_path, "data-turbolinks": false do %><span class="lnr lnr-magic-wand"></span><% end %></li>
          <li><a href="#"><span class="lnr lnr-sync"></span></a></li>
          <li><a><span class="lnr lnr-chevron-down btn_non_collapse" id="category_btn_collapse"></span></a></li>
        </ul>
        <div class="clear"></div>
      </div>
      <div id="category_collapse">
        <div class="nate">
          <ul class="list-inline tkab">
            <li class="id"><span>ID</span></li>
            <li class="namue"><span>NAME CATEGORY</span></li>
          </ul>
        </div>
        <div class="nate kone">
          <ul class="list-inline tkab">
            <% @categories.where('to_item = ?', 'graphic').limit(5).each_with_index do |foo, index| %>
              <li class="id <%= 'bg' if (index + 1)%2 == 1 %>"><span><%= index + 1 %></span></li>
              <li class="namue <%= 'bg' if (index + 1)%2 == 1 %>"><span><%= foo.name.upcase %></span></li>
            <% end %>
          </ul>
        </div>
      </div>
    </div>
  </div>
