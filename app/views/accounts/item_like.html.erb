                        <li><a href="#" class="hgfp"><span class="fa fa-folder"></span></a></li>
                        <li class="bvt"><a href="#" class="hgfp"><span class="fa fa-shopping-bag"></span></a></li>
                      </ul>
                    </div>
                    <div class="tooltip_j">
                      <div id="dat_tooltip">
                        <div class="img_o">
                          <% item.screenshots[0..0].each do |image| %>
                            <%= image_tag image.screenshot_url(:hover) %>
                          <% end %>  
                        </div>
                        <div class="titlw pull-left">
                          <h4 class="je"><%= truncate(item.name, length: 50) %></h4>
                          <span class="by"><%= item.user.company %> | </span> 
                          <span class="by">themes / <%= item.category.name %></span>
                        </div>
                        <div class="prce pull-right">
                          <span class="k">$<%= item.price.to_i.floor %></span>
                        </div>
                        <div class="clear"></div>
                      </div>
                    </div>
                  </div>
                </div>
                <div class="info">
                  <ul class="list-inline lf pull-left">
                    <li>
                      <span class="icon-cart_3 icon_cart_3 js"></span><span class="dr"><%= item.sell %></span>
