                      <%= link_to "niputuintanpermatasari", item_path(item), class: "trewk" %>
                      <ul class="mbfd">
                        <li>
                          <% if current_user.liked? item %>
                            <%= link_to dislike_theme_path(item.slug), remote: true, method: :put, class: "hgfp red", id: "dislike_#{item.id}" do %>
                              <i class="fa fa-heart"></i>
                            <% end %>
                          <% else %>
                            <%= link_to like_theme_path(item.slug), remote: true, method: :put, class: "hgfp", id: "like_#{item.id}" do %>
                              <i class="fa fa-heart"></i>
                            <% end %>
