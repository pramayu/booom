<% unless theme.aggree? || theme.reject? %> 
<div class="col-md-3 klie" id="klo_<%= theme.id %>">
	<div class="klhj867">
		<%= image_tag theme.thumbnail_url(:thumbnail) %>
		<div class="clkj878">
			<a id="del_to_pro_<%= theme.id %>" class="del_to_pro">
				<i class="fa fa-times" aria-hidden="true"></i>
			</a>
		</div>
	</div>
</div>
<% end %> 
<script>
	$('#del_to_pro_<%= theme.id %>').click(function(){
		$.ajax({
			type: "GET",
			url: "<%= del_to_pro_path(theme) %>",
			success: function(){
				$('#klo_<%= theme.id %>').hide("slow");
			}
		});
	});
</script>