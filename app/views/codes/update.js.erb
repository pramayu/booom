$('#srew_<%= @code.id %>').hide("slow");
$('#reve').modal("hide");

<% if @code.thumbnail_previously_changed? %>
	$('.img_tre43').hide("fast", function(){
		$(this).remove();
	});
	$("#jreg<%= @code.id %>").html("<%= escape_javascript render(partial: 'codes/thumbnail', locals: {code: @code}) %>");
<% end %>

<% @code.screenshots.each_with_index do |sfcd, index| %>
	$('#cditem_<%= sfcd.id %>').remove();
	$("#beta2").append("<%= escape_javascript render(partial: 'codes/screenshot', locals: {sfcd: sfcd, index: index}) %>");
	$('.mata').on("change", function(){
		$(this).removeClass('active');
	});
<% end %>

<% @code.brokes.each do |broke| %>
	<% if broke.resources_previously_changed? %>
		$('#fhg656').hide();
		$('#hftu88').html("<%= escape_javascript render(partial: 'codes/delbroke', locals: {broke: broke}) %>");
		$('#hftu88').show();
		$('#rhrh75').html("<%= escape_javascript render(partial: 'codes/delbroke', locals: {broke: broke}) %>");
		$('.l1_').hide();
	<% end %>
<% end %>